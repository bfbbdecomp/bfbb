#ifndef _MSL_CMATH
#define _MSL_CMATH

#include "math.h"

namespace std
{
    inline float sinf(float x)
    {
        return (float)sin((double)x);
    }

    inline float cosf(float x)
    {
        return (float)cos((double)x);
    }

    inline float tanf(float x)
    {
        return (float)tan((double)x);
    }
}

#endif